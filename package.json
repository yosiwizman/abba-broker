{
  "name": "abba-broker",
  "version": "0.1.0",
  "private": true,
  "description": "ABBA Hosting Broker - Backend service for managed app publishing",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --check .",
    "lint:fix": "next lint --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
